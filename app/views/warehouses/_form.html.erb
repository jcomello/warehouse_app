<%= simple_form_for @warehouse do |f| %>
  <%= f.error_notification %>
  <div class="form-inputs">
    <%= f.input :street %>
    <%= f.input :number %>
    <%= f.input :complement %>
    <%= f.input :cep %>
  </div>
  <div class="form-actions">
    <%= f.button :submit, value: t('buttons.save') %>
    <%= link_to t('navigation.back'), providers_path %>
  </div>
<% end %>
